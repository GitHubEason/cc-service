{
  "name": "cc-backend-ts",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "test": "jest --coverage",
    "dev": "ts-node-dev --respawn --pretty --transpile-only index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "pre-commit": "npm run  prettier && npm run lint && npm run typecheck && yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "git add ."
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "git add ."
    ]
  },
  "keywords": [],
  "author": "GitHubEason <defend.sen@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@shelf/jest-mongodb": "^2.2.2",
    "@types/eslint": "^8.4.2",
    "@types/http-status": "^1.1.2",
    "@types/prettier": "^2.6.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint": "^8.15.0",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "http-status": "^1.5.2",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.5.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.31",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.2.2",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "morgan": "^1.10.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
